nt":1,"parameters":[1,101,0,25,2]},{"code":235,"indent":2,"parameters":[3]},{"code":355,"indent":2,"parameters":["$gameVariables.setValue(101,100);"]},{"code":113,"indent":2,"parameters":[]},{"code":0,"indent":2,"parameters":[]},{"code":412,"indent":1,"parameters":[]},{"code":0,"indent":1,"parameters":[]},{"code":413,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[60]},{"code":241,"indent":0,"parameters":[{"name":"peaceful","volume":90,"pitch":100,"pan":0}]},{"code":230,"indent":0,"parameters":[120]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    这个小镇名叫蛇生镇，位于这个国家的北端，三牙山的包"]},{"code":401,"indent":0,"parameters":["围之中。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    受白牙山的影响，这里终年寒冷，即使盛夏也偶有飞雪。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    小镇上常见的树木是樱花树和枫树，虽然气候寒冷，它们"]},{"code":401,"indent":0,"parameters":["的长势却出人意料地旺盛，常年飞舞的樱花和枫叶成为了这里"]},{"code":401,"indent":0,"parameters":["代表性的景观，常有旅人为此慕名而来。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    除了樱花和枫叶之外，小镇还有一个特别之处，那就是对"]},{"code":401,"indent":0,"parameters":["蛇的崇拜。蛇的地位在这里非常尊贵，人们世代祭拜着一只名"]},{"code":401,"indent":0,"parameters":["为“白心”的白蛇神，小镇上随处可见它的图腾和石像。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    传说在很久以前，在这一带居住着一只强大凶恶的荒鬼，"]},{"code":401,"indent":0,"parameters":["他奴役着这里的居民做他的苦力，每个月还要吃掉一名10岁的"]},{"code":401,"indent":0,"parameters":["孩子。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    直到白蛇神来到这里，将牙齿中的毒液滴入酒中毒死了荒"]},{"code":401,"indent":0,"parameters":["鬼，才把人们从那凄惨的生活中解救了出来。"]},{"code":401,"indent":0,"parameters":["    也正因此，人们才世代祭拜白蛇，表达心中的感谢。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    镇上最大的白蛇像摆放在镇北侧的白心殿中，每年从四面"]},{"code":401,"indent":0,"parameters":["八方来到这里参加灵试的人都能看到它。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    人们常常会带上一些传说中白蛇神最爱的柿饼作为贡品献"]},{"code":401,"indent":0,"parameters":["到石像前，祈祷白蛇神能够保佑他们通过灵试，从而得到一只"]},{"code":401,"indent":0,"parameters":["属于自己的式鬼。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    小时候，镇上每年的灵试都是我最期待的事情。即便如今"]},{"code":401,"indent":0,"parameters":["已经离开故乡七十多年了，年少时瞒着父亲偷溜到灵殿看比赛"]},{"code":401,"indent":0,"parameters":["的事情还清晰地印在脑海里。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    曾经，我以为我总有一天也会像那些获胜的人一样，得到"]},{"code":401,"indent":0,"parameters":["属于自己的式鬼，然后随心所欲地调教和驱使他们。每当想到"]},{"code":401,"indent":0,"parameters":["这些，年少的我都会激动得浑身发烫，甚至整夜都睡不着觉。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    那时的我完全没有想到，自己的命运，会在某一天被完全"]},{"code":401,"indent":0,"parameters":["改写。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["    那一天，我坠入了那片幽深的湖水之中——"]},{"code":242,"indent":0,"parameters":[5]},{"code":221,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[120]},{"code":250,"indent":0,"parameters":[{"name":"coach","volume":90,"pitch":100,"pan":0}]},{"code":230,"indent":0,"parameters":[240]},{"code":250,"indent":0,"parameters":[{"name":"horse","volume":90,"pitch":100,"pan":0}]},{"code":230,"indent":0,"parameters":[120]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["车夫："]},{"code":401,"indent":0,"parameters":["别睡了，小哥，我们到啦！"]},{"code":101,"indent":0,"parameters":["wenzhu",0,0,2]},{"code":401,"indent":0,"parameters":["文助："]},{"code":401,"indent":0,"parameters":["……"]},{"code":101,"indent":0,"parameters":["wenzhu",0,0,2]},{"code":401,"indent":0,"parameters":["文助："]},{"code":401,"indent":0,"parameters":["（醒来）嗯……"]},{"code":401,"indent":0,"parameters":["\\|哦……\\|到了吗……"]},{"code":401,"indent":0,"parameters":["\\|多谢，一路上辛苦了……"]},{"code":250,"indent":0,"parameters":[{"name":"Move1","volume":90,"pitch":100